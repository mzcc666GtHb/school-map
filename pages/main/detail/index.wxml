<view class="container">
    <view class="detail-page-body">
        <view class="page-section page-section-spacing swiper">
            <swiper indicator-dots="{{indicatorDots}}"
                    style="height: 260px"
                    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
                <block wx:for="{{background}}" wx:key="*this">
                    <swiper-item>
                        <view class="swiper-item">
                            <image  style="width: 100%; height: 100%;" mode="aspectFill" src="{{item}}"></image>
                        </view>
                    </swiper-item>
                </block>
            </swiper>
            <view class="swiper-section-btns">
                <view class="section-btn">
                    <view class="section-icon">
                        <image style="width: 100%; height: 100%;" mode="aspectFill" src="../image/pliocy.svg"></image>
                    </view>
                    <view>
                        政策
                    </view>
                </view>
                <view class="section-btn">
                    <view class="section-icon">
                        <image style="width: 100%; height: 100%;" mode="aspectFill" src="../image/zsjz.svg"></image>
                    </view>
                    <view>
                        招生简章
                    </view>
                </view>
            </view>
        </view>
        <view class="brief-wrap">
            <view class="top-brief">
                <view class="top-brief-title">
                    <view class="brief-name">
                        上海市杨浦区复旦科技园小学
                    </view>
                    <view class="brief-distance">
                        <view class="brief-distance-icon">
                            <image style="width:100%;height:100%" mode="aspectFill" src="../image/loc.svg"></image>
                        </view>
                        <view>距离您的位置2.33km</view>
                    </view>
                </view>
                <view class="biref-tip">公</view>
            </view>
            <view class="biref-tag-cells">
                <view class="biref-tag-cell color1">一年级</view>
                <view class="biref-tag-cell color2">一年级</view>
                <view class="biref-tag-cell color3">一年级</view>
                <view class="biref-tag-cell color4">一年级</view>
                <view class="biref-tag-cell color5">一年级</view>
                <view class="biref-tag-cell color6">一年级</view>
            </view>
        </view>
