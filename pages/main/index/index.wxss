page {
  background: #F7F8FA;
}
page .title {
  margin-top: 20rpx;
  font-size: 36rpx;
  font-weight: bold;
}
.cover-view {
  position: absolute;
  width: 700rpx;
  height: 200rpx;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  /* background-color: #fff; */
  border-radius: 6px;
}
.index-map{
  width: 100%;
  height: 100vh;
}

.scroll-container{
  height: 100vh;
}
.scroll-view-item{
  color: #666;
  height: 180rpx;
  line-height: 180rpx;
  text-align: center;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
}

.house-wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.house-block {
  width: 600rpx;
  height: 800rpx;
  overflow-y: scroll;
  background-color: #fff;
}
.right-btns-wrap{
  position: fixed;
  right: 30rpx;
  bottom: 300rpx;
}
.right-btn{
  background-color: #fff;
  box-shadow: 0 1px 2px 1px rgba(1,1,1,.2);
  border-radius: 8px;
  width: 64rpx;
  height: 60rpx;
  margin-top: 16rpx;
}
.right-btn image{
  width: 64rpx;
  height: 64rpx;
  display: block;
}
.tab-input{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #666;
  text-align: center;
  background-color: #fff;
}
.container{
  overflow:hidden;
}
.container .van-tabs__line {
  height: 200rpx;
  border-radius: 0;
  bottom: -3rpx;
  background-color: #fff;
}
.container .van-tabs__line::before {
 content: "";
 position: absolute;
 background-color: #07c160;
 width: 40rpx;
 bottom: 10rpx;
 height: 4rpx;
 left: 50%;
 margin-left: -20rpx;
}
.container .van-tab {
  z-index: 2;
}
.container .van-tabs__scroll {
  background-color: rgba(1,1,1,.6);
}
.container .van-tabs {
  border-top-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
  overflow:hidden;
}
.container {
  --tabs-line-height: 38px;
}
.customCallout {
  box-sizing: border-box;
  background-color: #fff; 
  border:none;
  border-radius: 60px;
  height: 100px;
  width: 100px;
  padding: 6px;
  font-size: 12px;
  color: #fff;
  background-color:  #07c160;
  text-align: center;
}
